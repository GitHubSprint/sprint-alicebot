	<category>
		<pattern>Sprintbot zapisz kontekst pod nazwą *</pattern>
		<template> 
			<think>    
                <set name="contextname"><star/></set>
                 <save-context type="gpt" name="contextname"/>
            </think>
        </template>
    </category>

	<category>
		<pattern>Sprintbot przywróć kontekst o nazwie *</pattern>
		<template> 
			<think>    
                <set name="newcontextname"><star/></set>
                <set name="NowyKontekst">KONTEKST</set>
            </think>
        </template>
    </category>


